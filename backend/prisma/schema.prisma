model User {
  id         Int      @id @default(autoincrement())
  fullname   String
  email      String   @unique
  password   String
  created_at DateTime @default(now())
}

model Domain_Manager {
  id                 Int      @id @default(autoincrement())
  //domain
  domain             String   @unique
  ipaddress          String   @unique
  registrationdate   String?
  updateddate        String?
  expirydate         String?
  registrar          String
  takentime          String
  department         String
  //manager
  fullname           String
  designation        String
  manager_department String
  address            String
  contact_no         String
  email              String
  created_at         DateTime @default(now())
}

model Developer {
  id          Int      @id @default(autoincrement())
  fullname    String
  designation String
  department  String
  address     String
  contact_no  String
  email       String   @unique
  experience  String
  technology  String
  domain      String
  ipaddress   String
  created_at  DateTime @default(now())
}
